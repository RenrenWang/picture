		</div>
		<!--
        	弹窗
        -->
		<script src="/js/libs/jquery/jquery.js"></script>
		<script src="/js/libs/handlebars/handlebars.js"></script>
		<script src="/js/waterfall.min.js"></script>
		<script type="text/javascript" src="/js/app.js" ></script>
   <script type="text/javascript" src="/js/js.js" ></script>
	  <script type="text/javascript" src="/js/gridify-min.js" ></script>
		<script>



			$('#container').waterfall({
				itemCls: 'item',
				colWidth: 236,
				gutterWidth: 15,
				gutterHeight: 15,
				checkImagesLoaded: false,
	      maxPage: <%=maxPage%>,
       path: function(page) {
       return '/imgs/<%=type%>/ok/'+page;
		 }
			});



			var options =
			{
				srcNode: 'a',             // grid items (class, node)
				margin: '0px',             // margin in pixel, default: 0px
				width: '120px',             // grid item width in pixel, default: 220px
				max_width: '',              // dynamic gird item width if specified, (pixel)
				resizable: true,            // re-layout if window resize
				transition: 'all 0.5s ease' // support transition for CSS3, default: all 0.5s ease
			}
			$('.grid').gridify(options);

			Handlebars.registerHelper("changTime", function(t){
        return    changTime(t);
			})
		</script>

	</body>

</html>
